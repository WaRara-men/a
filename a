#coding=:UTE－8
  import discord
  from discord.ext import tasks
  TOKEN ＝ "バセヨ！" #トークン
  CHANNEL_ID ＝ 741646166030286879 #チャンネルID
  # 接続に必要なオブジェクトを生成
  client ＝ discord.Client()
  # 60秒に1回ループ
  tasks.loop(seconds＝60)
  async def loop():
  channel ＝ client.get_channel(CHANNEL_ID)
  await channel.send('バセヨ！')
  #ループ処理実行
  loop.start()
  # Bot起動とDiscordサーバーへの接続
  client.run(TOKEN)
